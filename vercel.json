{
    "version": 2,
    "functions": {
        "api/*.php": {
            "runtime": "vercel-php@0.7.1"
        }
    },
    "routes": [
        {
            "src": "/dist/(.*)",
            "dest": "api/dist/$1"
        },
        {
            "src": "/bower_components/(.*)",
            "dest": "api/bower_components/$1"
        },
        {
            "src": "/js/(.*)",
            "dest": "api/js/$1"
        },
        {
            "src": "/(.*)",
            "dest": "api/index.php"
        }
    ]
}